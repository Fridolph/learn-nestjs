{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es2017",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "baseUrl": "./",
      "incremental": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "noImplicitAny": false,
      "noImplicitReturns": false,
      "noImplicitThis": false,
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "removeComments": true,
      "strict": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true
   }
}